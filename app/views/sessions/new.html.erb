
<% if !logged_in? %>
<%= flash.notice %>

  <%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit 'log in' %>
  <% end %>

<%= button_to 'sign up', '/signup', method: 'get' %>
<% end %>
