<%= form_for @exercise do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :instructions %>
  <%= f.text_field :instructions %>

  <%= f.collection_select :category_id, Category.all, :id, :name  %>

  <br>
  <%= f.label :muscles %> <br>
  <%= f.collection_check_boxes :muscle_ids, Muscle.all, :id, :name %>
  <br>
  <%= f.submit %>

<% end %>
